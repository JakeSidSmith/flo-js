{
  "name": "slik",
  "version": "0.1.0",
  "description": "Animation library for use with HTML5 canvas",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && http-server examples -o -c-1",
    "watch-less": "watch 'npm run build-less' examples/src/less/",
    "watch-js": "watchify examples/src/js/index.js -o examples/build/js/index.js -v",
    "watch": "npm run watch-js & npm run watch-less",
    "build-less": "mkdir -p examples/build/css && lessc examples/src/less/index.less > examples/build/css/index.css && echo 'Less compiled'",
    "build-js": "mkdir -p examples/build/js && browserify examples/src/js/index.js -o examples/build/js/index.js",
    "build": "ulimit -n 512 && npm install && npm run build-less && npm run build-js",
    "lint": "eslint src/ && eslint -c .eslintrc-examples.json examples/src/js/",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JakeSidSmith/slik.git"
  },
  "keywords": [
    "animation",
    "tween",
    "flow",
    "slik",
    "smooth",
    "library",
    "html5",
    "canvas"
  ],
  "author": "Jake 'Sid' Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JakeSidSmith/slik/issues"
  },
  "homepage": "https://github.com/JakeSidSmith/slik",
  "contributors": [
    {
      "name": "Jake 'Sid' Smith",
      "url": "https://githib.com/JakeSidSmith"
    }
  ],
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "dependencies": {},
  "devDependencies": {
    "browserify": "14.1.0",
    "eslint-config-jakesidsmith": "git+https://github.com/JakeSidSmith/eslint-config-jakesidsmith.git#v2.1.0",
    "http-server": "0.9.0",
    "less": "2.7.2",
    "watch": "1.0.2",
    "watchify": "3.9.0"
  }
}
