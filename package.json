{
  "name": "slik",
  "version": "0.0.0",
  "description": "Animation library for use with HTML5 canvas",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && http-server examples -o -c-1 & npm run watch",
    "watch-less": "watch 'npm run build-less' examples/src/less/",
    "watch-js": "watchify -t babelify examples/src/js/index.js -o examples/build/js/index.js -v",
    "watch": "npm run watch-js & npm run watch-less",
    "build-less": "mkdir -p examples/build/css && lessc examples/src/less/index.less > examples/build/css/index.css && echo 'Less compiled'",
    "build-js": "mkdir -p examples/build/js && browserify -t babelify examples/src/js/index.js -o examples/build/js/index.js",
    "build": "ulimit -n 512 && npm install && npm run build-less && npm run build-js",
    "lint": "jshint 'src/' 'examples/src/js' && jscs 'src/' 'examples/src/js'",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JakeSidSmith/slik.git"
  },
  "keywords": [
    "animation",
    "tween",
    "flow",
    "slik",
    "smooth",
    "library",
    "html5",
    "canvas"
  ],
  "author": "Jake 'Sid' Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JakeSidSmith/slik/issues"
  },
  "homepage": "https://github.com/JakeSidSmith/slik",
  "contributors": [
    {
      "name": "Jake 'Sid' Smith",
      "url": "https://githib.com/JakeSidSmith"
    }
  ],
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "dependencies": {
  },
  "devDependencies": {
    "babelify": "6.1.3",
    "browserify": "11.0.0",
    "http-server": "0.8.0",
    "jscs": "1.13.1",
    "jshint": "2.8.0",
    "less": "2.5.1",
    "watch": "0.16.0",
    "watchify": "3.2.3"
  },
  "jshintConfig": {
    "browserify": true,
    "unused": true,
    "undef": true,
    "globals": {
      "console": false
    }
  },
  "jscsConfig": {
    "disallowMultipleVarDecl": "exceptUndefined",
    "validateQuoteMarks": {
      "mark": "'",
      "escape": false
    },
    "validateParameterSeparator": ", ",
    "validateIndentation": 2,
    "requireSpacesInFunction": {
      "beforeOpeningRoundBrace": true,
      "beforeOpeningCurlyBrace": true
    },
    "requireSpacesInConditionalExpression": {
      "afterTest": true,
      "beforeConsequent": true,
      "afterConsequent": true,
      "beforeAlternate": true
    },
    "disallowTrailingWhitespace": true,
    "maximumLineLength": 100,
    "requireCurlyBraces": [
      "if",
      "else",
      "for",
      "while",
      "do",
      "try",
      "catch"
    ],
    "disallowMixedSpacesAndTabs": true,
    "requireSpaceBeforeBinaryOperators": true,
    "safeContextKeyword": [
      "self"
    ],
    "disallowMultipleLineBreaks": true,
    "disallowMultipleLineStrings": true,
    "disallowSpaceAfterObjectKeys": true,
    "disallowNewlineBeforeBlockStatements": true,
    "disallowTrailingComma": true,
    "requireCommaBeforeLineBreak": true,
    "requireSpaceBetweenArguments": true,
    "requireSpaceBeforeBlockStatements": true,
    "requireSpaceBeforeObjectValues": true,
    "requireSpacesInForStatement": true
  }
}
